<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidtutorialshub.Emergency_call">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/untitled"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.androidtutorialshub.Emergency_call.activities.TheftPage"></activity>
        <activity android:name="com.androidtutorialshub.Emergency_call.activities.AccidentPage" />
        <activity android:name="com.androidtutorialshub.Emergency_call.activities.Disasterpage" />
        <activity android:name="com.androidtutorialshub.Emergency_call.activities.FirePage" />
        <activity android:name="com.androidtutorialshub.Emergency_call.activities.WomenPage" />
        <activity
            android:name="com.androidtutorialshub.Emergency_call.activities.MainPageApp"
            android:theme="@style/MenuActivity6Theme" />
        <activity android:name="com.androidtutorialshub.Emergency_call.activities.UserDetails" />
        <activity
            android:name="com.androidtutorialshub.Emergency_call.activities.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.androidtutorialshub.Emergency_call.activities.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.androidtutorialshub.Emergency_call.activities.RegisterActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>